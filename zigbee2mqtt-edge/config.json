{
  "name": "Zigbee2MQTT Piętro",
  "slug": "zigbee2mqtt_pietro",
  "description": "Development build of the Zigbee2MQTT add-on for piętro",
  "version": "edge",
  "startup": "services",
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": false,
  "host_network": true,
  "apparmor": false,
  "ingress": true,
  "ingress_port": 8098,
  "panel_icon": "mdi:zigbee",
  "panel_title": "Z2MQTT Piętro",
  "arch": ["aarch64","amd64","armv7","i386"],
  "options": {},
  "schema": {},
  "map": [
    { "type": "homeassistant_config", "path": "/config" },
    { "type": "share", "path": "/share" },
    { "type": "addon_config", "path": "/addon_config" }
  ],
  "ports": {
    "8098/tcp": 8098
  },
  "ports_description": {
    "8098/tcp": "Ingress front-end port"
  },
  "image": "ghcr.io/zigbee2mqtt/zigbee2mqtt-edge-{arch}"
}
